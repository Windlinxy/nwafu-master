<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="pers.nwafumaster.mapper.MessageMapper">

    <resultMap id="BaseResultMap" type="pers.nwafumaster.beans.Message">
            <id property="messageId" column="message_id" jdbcType="INTEGER"/>
            <result property="userId" column="user_id" jdbcType="INTEGER"/>
            <result property="messageType" column="message_type" jdbcType="CHAR"/>
            <result property="title" column="title" jdbcType="VARCHAR"/>
            <result property="content" column="content" jdbcType="VARCHAR"/>
            <result property="fileUrl" column="file_url" jdbcType="VARCHAR"/>
    </resultMap>

    <sql id="Base_Column_List">
        message_id,user_id,message_type,
        title,content,file_url
    </sql>
</mapper>
